{
	"extends":  [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
	"parser": "babel-eslint",
	"plugins": [
		"react" 
	],
	"rules": {
		"no-unused-vars": 0,
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".tsx"] }],
		"no-tabs": "off",
		"editor.tabSize": "4",
		"indent": [2, "tab", { "SwitchCase": 1 }],
		"no-mixed-spaces-and-tabs": [2, false],
		"editor.insertSpaces": true,
		"no-trailing-spaces": 2,
		"default-case": 2,
		"no-redeclare": 2,
		"semi": [2, "always"],
		"no-extra-semi": 2,
		"semi-spacing": [2, {"before": false, "after": true}],
		"consistent-this": [1, "_that"],
		"jsx-quotes": [2, "prefer-double"],
		"no-eval": 1,
		"no-func-assign": 2,
		"no-fallthrough": 2,
		"no-var": 0,
		"no-with": 2,
		"eqeqeq": 2,
		"strict": 2,
		"no-undef": 0,
		"no-console": 0,
		"no-unreachable": 0,
		"no-spaced-func": 2,
		"no-multiple-empty-lines": [2, {"max": 2}],
		"no-new-object": 2,
		"no-extra-parens": 1
	},
	"settings": {
		"import/ignore": [
		  "node_modules"
		]
	}
}
